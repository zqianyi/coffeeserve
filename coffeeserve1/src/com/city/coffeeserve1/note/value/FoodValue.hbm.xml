<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.city.coffeeserve1.note.value" >
    <class name="FoodValue" table="food">
        <id name="fid" column="fid" >
         <generator class="native" />
        </id>
        
        <property name="fname" column="fname"/>
        <property name="fkind"  column="fkind"/>
        <property name="fstock" column="fstock"/>  
         <property name="fprice" column="fprice"/> 
           <property name="photo"  column="photo"/>
        <property name="fileName"  column="fileName"/>
        <property name="contentType"  column="contentType"/>
                 
    </class>
</hibernate-mapping>

